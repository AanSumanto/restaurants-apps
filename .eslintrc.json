{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true,
        "codeceptjs/codeceptjs": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "no-undef": 0,
        "linebreak-style": 0,
        "no-unused-expressions": 0,
        "no-underscore-dangle": 0,
        "eol-last": 0,
        "indent": 0,
        "no-unused-vars": 0,
        "no-else-return": 0,
        "import/extensions": 0,
        "import/no-extraneous-dependencies": 0,
        "no-restricted-globals": 0,
        "no-prototype-builtins": 0,
        "consistent-return": 0,
        "import/prefer-default-export": 0,
        "no-useless-return": 0,
        "eslint-disable no-return-assign": 0,
        "no-empty-function": 0,
        "class-methods-use-this": 0,
        "no-return-assign": 0,
        "no-new": 0,
        "import/no-named-as-default-member": 0,
        "no-plusplus": 0,
        "no-await-in-loop": 0
    },
    "plugins": ["jasmine", "codeceptjs"]
}
